{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 400,
    "data": {
        "url": "deez.csv"
    },
    "transform": [
        {
            "filter": "datum.tests >= 0"
        },
        {
            "filter": "datum.positives >= 0"
        },
        {
            "calculate": "timeParse(datum['date'], '%b-%y')",
            "as": "date1"
        }
    ],
    "mark": "circle",
    "encoding": {
        "x": {
            "field": "date1",

            "title": "Date"
        },
        "y": {
            "field": "tests",
            "type": "quantitative",
            "title": "Number of tests"
        },
        "opacity":{
            "value": 0.5
        },
        "size": {
            "field": "positive_rate",
            "type": "quantitative",
            "title": "% Positive Rate"
        }
    }
}